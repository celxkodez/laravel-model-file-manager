    "description": "manage file upload on model fields",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Celxkodez\\LaravelModelFileManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Celestine",
